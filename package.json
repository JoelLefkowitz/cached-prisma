{
  "name": "cached-prisma",
  "description": "A Prisma client abstraction that simplifies caching.",
  "version": "1.3.0",
  "private": false,
  "license": "MIT",
  "type": "module",
  "author": "Joel Lefkowitz",
  "repository": "https://github.com/joellefkowitz/cached-prisma",
  "homepage": "https://joellefkowitz.github.io/cached-prisma",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "prisma",
    "caching"
  ],
  "metadata": {
    "publisher": "npm",
    "languages": [
      "TypeScript"
    ],
    "frameworks": [
      "Prisma"
    ],
    "paradigms": [
      "Object oriented"
    ],
    "lifecycle": "Alpha"
  },
  "scripts": {
    "prepare": "husky",
    "lint": "cspell . --dot && eslint . --fix && tsc --noEmit",
    "format": "prettier . --write",
    "build": "tsup",
    "test": "jest",
    "docs": "typedoc"
  },
  "peerDependencies": {
    "@prisma/client": "5.9.1",
    "prisma": "^5.9.1"
  },
  "dependencies": {
    "@types/memcached": "^2.2.10",
    "collections": "^5.1.12",
    "memcached": "^2.2.2"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.41.0",
    "@types/jest": "29.5.12",
    "@types/node": "^20.11.23",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "cspell": "^8.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "husky": "^9.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "tsup": "^8.0.2",
    "typedoc": "^0.25.7",
    "typedoc-material-theme": "^1.0.2",
    "typescript": "^5.3.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true
  }
}